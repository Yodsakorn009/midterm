<% include ../partials/header %>
<div class="container">
<h1>Add Team</h1> 
<h4><%= soc.League %> <%= soc.Season %></h4>
  <form action="/editdeatilteam" method="POST">
    <div class="form-group">
            <input type="hidden" class="form-control" value="<%= soc.id %>" name="socid" id="" aria-describedby="helpId"
            placeholder="">
      <label for="">ID</label>
      <input type="text" class="form-control" value="" name="ID" id="" aria-describedby="helpId"
        placeholder="">
      <small id="helpId" class="form-text text-muted"></small>
    </div>
    <div class="form-group">
      <label for="">Name</label>
      <input type="text" class="form-control" value="" name="Name" id=""
        aria-describedby="helpId" placeholder="">
      <small id="helpId" class="form-text text-muted"></small>
    </div>
    <div class="form-group">
      <label for="">Location</label>
      <input type="text" class="form-control" value="" name="Location" id="" aria-describedby="helpId"
        placeholder="">
      <small id="helpId" class="form-text text-muted"></small>
    </div>
    <div class="form-group">
        <label for="">Stadium</label>
        <input type="text" class="form-control" value="" name="Stadium" id="" aria-describedby="helpId"
          placeholder="">
        <small id="helpId" class="form-text text-muted"></small>
      </div>
     <button type="submit" class="btn btn-primary">Add</button>
  </form>
</div>
<% include ../partials/footer %>