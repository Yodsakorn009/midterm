      <label for="">ID</label>
      <input type="text" class="form-control" value="<%= soc.id %>" readonly name="ID"  aria-describedby="helpId"
        placeholder="">  
    </div>
    <div class="form-group">
      <label for="">League</label>
      <input type="text" class="form-control" value="<%= soc.League %>" name="League" id="" aria-describedby="helpId" placeholder="">
          </div>
    <div class="form-group">
      <label for="">Season</label>
      <input type="text" class="form-control" value="<%= soc.Season %>" name="Season" id="" aria-describedby="helpId"
        placeholder="">
        </div>    <center>
    <button type="submit" class="btn btn-primary">edit</button>
    <a class="btn btn-danger" href="/soccer" role="button">cancel</a></center>
  </form>
</div>
