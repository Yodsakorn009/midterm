<% include ../partials/header %>

<div class="container"><br>
    <h1><%= soc.League %> <%= soc.Season %></h1>
    <table class="table">
        <div class="form-group">
            <label for="">ID</label>
            <input type="text" value="<%= soc.Team[key].id %>" readonly class="form-control" name="" id=""
                aria-describedby="helpId" placeholder="">
            <small id="helpId" class="form-text text-muted"></small>
        </div>
        <div class="form-group">
            <label for="">Name Team</label>
            <input type="text" value="<%= soc.Team[key].Name %>"readonly class="form-control" name="" id=""
                aria-describedby="helpId" placeholder="">
            <small id="helpId" class="form-text text-muted"></small>
        </div>
        <div class="form-group">
            <label for="">Location</label>
            <input type="text" value="<%= soc.Team[key].Location %>" readonly class="form-control" name="" id=""
                aria-describedby="helpId" placeholder="">
            <small id="helpId" class="form-text text-muted"></small>
        </div>
        <div class="form-group">
            <label for="">Stadium</label>
            <input type="text" value="<%= soc.Team[key].Stadium %>" readonly class="form-control" name="" id=""
                aria-describedby="helpId" placeholder="">
            <small id="helpId" class="form-text text-muted"></small>
        </div>
        <div><center>
                <a class="btn btn-primary" href="/soccerview/<%= soc.id%>" role="button">back</a></center>
        </div>
    </table>

</div>
<% include ../partials/footer %>